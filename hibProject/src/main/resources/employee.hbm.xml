<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="org.jsp.hibernateproject.Employee" table="Emp">
<id name="id" column="id">
<generator class="identity"></generator>
</id>

<property name="name" column="name" not-null="true"></property>
<property name="phone" column="phone" not-null="true"></property>
<property name="email" column="email" not-null="true"></property>
<property name="designation" column="designation" not-null="true"></property>
<property name="salary" column="salary"></property>
<property name="password" column="password" not-null="true"></property>

</class>
</hibernate-mapping>